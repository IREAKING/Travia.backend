        "/storage/list": {
            "post": {
                "description": "List all images in the bucket",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Storage"
                ],
                "summary": "List images in the bucket",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Folder path",
                        "name": "folder_path",
